{
  "name": "netlify-cms-widget-pathname",
  "version": "0.0.0",
  "description": "netlify-cms widget to validate pathnames and display url's",
  "keywords": [
    "cms",
    "netlify",
    "netlify-cms",
    "pathname",
    "url",
    "widget"
  ],
  "homepage": "https://github.com/buz-zard/netlify-cms-widget-pathname#readme",
  "bugs": {
    "url": "https://github.com/buz-zard/netlify-cms-widget-pathname/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buz-zard/netlify-cms-widget-pathname.git"
  },
  "license": "MIT",
  "author": "Karolis Å arapnickis",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:demo": "webpack --display errors-only --devtool source-map --config webpack.dev.config.js",
    "build:package": "babel src --out-dir lib --source-maps",
    "clean": "rimraf public",
    "format": "prettier --write '**/*.*' && sort-package-json",
    "lint": "run-p lint:*",
    "lint:js": "eslint .",
    "lint:md": "markdownlint README.md",
    "qa": "npm run lint",
    "semantic-release": "semantic-release",
    "start": "webpack-serve --config webpack.dev.config.js --log-level error --open-path '/#/collections/test/entries/test'",
    "travis-deploy-once": "travis-deploy-once",
    "try-demo": "run-s clean build:demo && serve public",
    "upgrade-interactive": "npm-check --update"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run qa"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@k.sh/eslint-config": "^1.6.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "markdownlint-cli": "^0.13.0",
    "netlify-cms": "^2.2.1",
    "netlify-cms-ui-default": "^2.1.1",
    "netlify-cms-widget-string": "^2.0.4",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "react": "^16.6.3",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.12.3",
    "serve": "^10.1.1",
    "sort-package-json": "^1.16.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "travis-deploy-once": "^5.0.9",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.3"
  },
  "peerDependencies": {
    "netlify-cms-ui-default": "^2",
    "netlify-cms-widget-string": "^2",
    "prop-types": "^15",
    "react": "^16"
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json,babelrc}": [
      "pretty-quick --staged"
    ]
  }
}
